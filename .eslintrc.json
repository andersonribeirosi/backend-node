{
  "env": {
      "es2021": true,
      "node": true,
      "jest": true
  },
  "extends": [
      "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "plugins": [
      "@typescript-eslint"
  ],
  "rules": {
  },
  "no-use-before-define": ["error", {
    "functions": true,
    "classes": true,
    "variables": false,
    "allowNamedExports": false
}]
}
